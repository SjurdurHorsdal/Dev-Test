{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "npx sequelize-cli db:migrate && nodemon",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prettier": "prettier --config .prettierrc.json --write src/**/*.ts",
    "model-generate": "npx sequelize-cli model:generate --name test --attributes t:string",
    "mg-to": "npx sequelize-cli db:migrate --to",
    "undo-to": "npx sequelize-cli db:migrate:undo --name"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^8.5.1",
    "sequelize-cli": "^6.5.2",
    "sqlite3": "^5.1.2",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "sequelize": "^6.25.7",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  }
